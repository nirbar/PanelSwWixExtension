  <Fragment>
    <CustomAction Id="PSW_InstallUtilSched" SuppressModularization="yes" BinaryKey="PswManagedCA.dll" DllEntry="InstallUtilSched" Execute="immediate"/>
    <CustomAction Id="PSW_InstallUtil_InstallExec" SuppressModularization="yes" BinaryKey="PswManagedCA.dll" DllEntry="InstallUtilExec" Execute="deferred" Impersonate="no"/>
    <CustomAction Id="PSW_InstallUtil_InstallRollback" SuppressModularization="yes" BinaryKey="PswManagedCA.dll" DllEntry="InstallUtilExec" Execute="rollback" Impersonate="no"/>
    <CustomAction Id="PSW_InstallUtil_UninstallExec" SuppressModularization="yes" BinaryKey="PswManagedCA.dll" DllEntry="InstallUtilExec" Execute="deferred" Impersonate="no"/>
    <CustomAction Id="PSW_InstallUtil_UninstallRollback" SuppressModularization="yes" BinaryKey="PswManagedCA.dll" DllEntry="InstallUtilExec" Execute="rollback" Impersonate="no"/>

    <InstallExecuteSequence>
      <Custom Action="PSW_InstallUtilSched" After="InstallInitialize" Overridable="yes"/>

      <Custom Action="PSW_InstallUtil_InstallRollback" Before="PSW_InstallUtil_InstallExec" Overridable="yes"/>
      <Custom Action="PSW_InstallUtil_InstallExec" Before="InstallServices" Overridable="yes"/>

      <Custom Action="PSW_InstallUtil_UninstallRollback" Before="PSW_InstallUtil_UninstallExec" Overridable="yes"/>
      <Custom Action="PSW_InstallUtil_UninstallExec" Before="DeleteServices" Overridable="yes"/>
    </InstallExecuteSequence>
    <UI>
      <ProgressText Action="PSW_InstallUtil_InstallExec">!(loc.InstallUtil)</ProgressText>
      <ProgressText Action="PSW_InstallUtil_InstallRollback">!(loc.InstallUtil)</ProgressText>
      <ProgressText Action="PSW_InstallUtil_UninstallExec">!(loc.InstallUtil)</ProgressText>
      <ProgressText Action="PSW_InstallUtil_UninstallRollback">!(loc.InstallUtil)</ProgressText>
    </UI>
  </Fragment>

  <!-- SqlSearch -->
  <Fragment>
    <CustomAction Id="SqlSearch" BinaryKey="PanelSwCustomActions.dll" DllEntry="SqlSearch" Execute="firstSequence" />
    <InstallExecuteSequence>
      <Custom Action="SqlSearch" After="AppSearch" Overridable="yes"/>
    </InstallExecuteSequence>
    <InstallUISequence>
      <Custom Action="SqlSearch" After="AppSearch" Overridable="yes"/>
    </InstallUISequence>
    <UI>
      <ProgressText Action="SqlSearch">!(loc.SqlSearch)</ProgressText>
    </UI>
  </Fragment>

  <!-- BackupAndRestore -->
  <Fragment>
    <CustomAction Id="BackupAndRestore" SuppressModularization="yes" BinaryKey="PanelSwCustomActions.dll" DllEntry="BackupAndRestore" Execute="immediate" Return="check" />
    <CustomAction Id="BackupAndRestore_rollback" SuppressModularization="yes" BinaryKey="PanelSwCustomActions.dll" DllEntry="CommonDeferred" Execute="rollback" Impersonate="no" />
    <CustomAction Id="BackupAndRestore_deferred" SuppressModularization="yes" BinaryKey="PanelSwCustomActions.dll" DllEntry="CommonDeferred" Execute="deferred" Impersonate="no" />
    <CustomAction Id="BackupAndRestore_commit" SuppressModularization="yes" BinaryKey="PanelSwCustomActions.dll" DllEntry="CommonDeferred" Execute="commit" Return="ignore" Impersonate="no" />

    <InstallExecuteSequence>
      <Custom Action="BackupAndRestore" Before="RemoveExistingProducts" Overridable="yes"/>
      <Custom Action="BackupAndRestore_rollback" After="InstallInitialize" Overridable="yes"/>
      <Custom Action="BackupAndRestore_deferred" After="InstallInitialize" Overridable="yes"/>
      <Custom Action="BackupAndRestore_commit" After="InstallInitialize" Overridable="yes"/>
    </InstallExecuteSequence>

    <UI>
      <ProgressText Action="BackupAndRestore">!(loc.BackupAndRestore)</ProgressText>
      <ProgressText Action="BackupAndRestore_rollback">!(loc.BackupAndRestore)</ProgressText>
      <ProgressText Action="BackupAndRestore_deferred">!(loc.BackupAndRestore)</ProgressText>
      <ProgressText Action="BackupAndRestore_commit">!(loc.BackupAndRestore)</ProgressText>
    </UI>
  </Fragment>

</Wix>