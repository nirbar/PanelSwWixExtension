      <Custom Action="Test2" After="Md5Hash"><![CDATA[TEST_2<>"5A20070785F438E51DF985FFC8C37689"]]></Custom>
      <Custom Action="Test3" After="Md5Hash"><![CDATA[TEST_3]]></Custom>
