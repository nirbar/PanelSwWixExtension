<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi" xmlns:PanelSW="http://schemas.panel-sw.co.il/wix/WixExtension" xmlns:dep="http://schemas.microsoft.com/wix/DependencyExtension"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.panel-sw.co.il/wix/WixExtension ../../PanelSwWixExtension/XSD/PanelSwWixExtension.xsd">
  
	<Bundle Name="UninstallCommandUT" Version="$(var.JetVersion)" Manufacturer="$(var.JetManufacturer)" UpgradeCode="f37b5c48-dd17-4ff6-af58-1bd697c2c1d7">
    <BootstrapperApplicationRef Id="WixStandardBootstrapperApplication.RtfLicense"/>
    
		<Chain ParallelCache="yes" DisableSystemRestore="yes">

      <ExePackage SourceFile="$(var.DeferredExePackage.TargetPath)" 
                  InstallCommand="--ignore-me">
        <dep:Provides DisplayName="test" Key="{16ABC68D-BB52-458C-A8F6-F5D5CC3D6C39}" Version="!(bind.packageVersion.DeferredExePackage.exe)"/>
        <PanelSW:UninstallCommand DetectCondition="WixBundleInstalled" CommandLine='"[MyExe]" /uninstall /silent /norestart /log "[WixBundleLog_DeferredExePackage.exe_uninstall]"'/>
      </ExePackage>
    </Chain>
	</Bundle>
</Wix>
